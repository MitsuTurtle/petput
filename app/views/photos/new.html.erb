<div class="photo-new-page">
  <div class="photo-new">
    <div>
      <%= link_to image_tag("petput-logo.png", class:"photo-new-petput-icon"), photos_path %>
      <h2 class="photo-new-title">ペット写真を投稿</h2>


      <div class="form">
        <%# <div class="click-home"> %>
        <%#= link_to '戻る', :back, class: "home-button" %>
        <%# </div> %>

        <%= form_with model: @photo, local: true do |f| %>

        <%= render partial: "shared/form", locals: {f: f} %>


        <div class="click-submit">
          <%= f.submit "シェア", class: "submit-button" %>
        </div>
        <% end %>
      </div>
    </div>
  </div>